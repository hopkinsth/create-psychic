{
  "name": "api",
  "version": "0.0.0",
  "description": "api",
  "main": "src/main.ts",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "routes": "yarn build && NODE_ENV=development ts-node -r tsconfig-paths/register ./src/bin/routes.ts",
    "build-howl-dir": "node ./node_modules/howl/bin/build-howl-dir",
    "build": "yarn build-howl-dir && tsc && yarn build-globals && tsc && cp src/conf/app.yml dist/src/conf/app.yml",
    "build-globals": "ts-node -r tsconfig-paths/register ./src/.howl/buildGlobals.ts",
    "prod": "yarn build && NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/app.js",
    "dev": "yarn build && NODE_ENV=development ts-node -r tsconfig-paths/register ./src/main.ts",
    "db": "yarn build && NODE_PATH=src yarn sequelize",
    "db:migrate": "yarn build && NODE_PATH=src yarn sequelize db:migrate",
    "db:rollback": "yarn build && NODE_PATH=src yarn sequelize db:rollback",
    "db:drop": "yarn build && NODE_PATH=src yarn sequelize db:drop",
    "db:create": "yarn build && NODE_PATH=src yarn sequelize db:create",
    "g:migration": "NODE_PATH=src yarn db migration:create",
    "prespec-server": "yarn build",
    "spec-server": "NODE_ENV=test ts-node -r tsconfig-paths/register ./src/spec-server.ts",
    "spec": "yarn uspec && yarn fspec",
    "uspec": "yarn build && jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "fspec": "yarn build && jest --config=./spec/features/jest.config.js --runInBand --forceExit",
    "console": "NODE_ENV=development ts-node -r tsconfig-paths/register --project ./tsconfig.json ./src/conf/repl.ts",
    "c": "yarn console"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/expect-puppeteer": "^5.0.2",
    "@types/express": "^4.17.15",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/puppeteer": "^7.0.4",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest-puppeteer": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "node-sass": "^8.0.0",
    "pg": "^8.8.0",
    "puppeteer": "^19.6.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.0",
    "sequelize-typescript": "^2.1.5",
    "superagent": "^8.0.8",
    "typeorm": "^0.3.11",
    "howl": "https://github.com/avocadojesus/howl.git#26a1a18"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/jest-plugin-context": "^2.9.5",
    "jest": "^29.4.2",
    "jest-plugin-context": "^2.9.0",
    "sinon": "^15.0.1",
    "supertest": "^6.3.3",
    "supertest-session": "^4.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
