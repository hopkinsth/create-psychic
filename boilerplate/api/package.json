{
  "name": "create-psychic-app",
  "version": "0.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/avocadojesus/create-psychic-app.git",
  "author": "avocadojesus <jgarbey@gmail.com>",
  "license": "Apache 2.09",
  "private": true,
  "scripts": {
    "psy": "yarn --cwd=node_modules/psychic psy",
    "dream": "yarn --cwd=node_modules/dream dream",
    "dev": "NODE_ENV=development ts-node ./src/main.ts",
    "sync": "yarn psy sync:all && yarn dream sync:existing",
    "uspec": "yarn sync && jest --config=./spec/unit/jest.config.js --runInBand --forceExit",
    "fspec": "jest --config=./spec/features/jest.config.js --runInBand --forceExit",
    "postinstall": "yarn psy clean && yarn dream sync:existing"
  },
  "dependencies": {
    "psychic": "https://github.com/avocadojesus/psychic.git#0d454f6"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^5.0.3",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/supertest": "^2.0.12",
    "jest-puppeteer": "^8.0.6",
    "puppeteer": "^19.9.1",
    "supertest": "^6.3.3"
  }
}
